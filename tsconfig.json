{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "es5",
      "lib": ["es5", "es6", "dom"],
      "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
      "./src/**/*"
  ]
}